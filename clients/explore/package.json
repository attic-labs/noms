{
  "name": "noms-explorer",
  "dependencies": {
    "react": "^0.12.0",
    "immutable": "^3.7.4"
  },
  "devDependencies": {
    "babelify": "^6.1.3",
    "browserify": "^6.2.0",
    "uglify-js": "~2.4.15",
    "watchify": "^2.1.1"
  },
  "scripts": {
    "start": "watchify -o explore.js -t babelify -v -d main.js",
    "build": "NODE_ENV=production browserify main.js -t babelify | uglifyjs -cm > explore.js"
  }
}
