  "dependencies": {
    "@attic/noms": "file:../../noms",
    "@attic/webpack-config": "^2.2.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-noms": "1.0.1",
    "eslint": "^3.13.1",
    "eslint-config-noms": "1.0.0",
    "flow-bin": "^0.38.0",
    "webpack": "^1.14.0"
  }
}
